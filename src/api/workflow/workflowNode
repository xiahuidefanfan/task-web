import request from '@/utils/request'

/**
 * 查询节点
 * @param params
 */
export function list( params ) {
  return request ({
    url: '/task/workflowNode/list.action',
    method: 'post',
    params
  })
}

/**
 * 添加节点
 * @param params
 */
export function add( params ) {
  return request ({
    url: '/task/workflowNode/add.action',
    method: 'post',
    params
  })
}

/**
 * 修改节点
 * @param params
 */
export function update( params ) {
  return request ({
    url: '/task/workflowNode/update.action',
    method: 'put',
    params
  })
}